<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
	<entity class="com.catify.persistence.beans.NodeCache">
		<named-query name="nodeCache_LoadByKey">
			<query>SELECT a FROM NodeCache a WHERE a.beanKey = :param</query>
		</named-query>
		<named-query name="nodeCache_LoadAllKeys">
			<query>SELECT a.beanKey FROM NodeCache a</query>
		</named-query>
		<attributes>
			<id name="id">
				<generated-value generator="system-uuid" />
			</id>
			<one-to-one name="beanValue">
				<cascade>
					<cascade-all />
				</cascade>
			</one-to-one>
			<basic name="beanKey" optional="false" />
		</attributes>
	</entity>
	<entity class="com.catify.core.event.impl.beans.StateEvent">
		<attributes>
			<id name="id">
				<generated-value generator="system-uuid" />
			</id>
			<basic name="instanceId" optional="false" />
			<basic name="state" optional="false" />
		</attributes>
	</entity>
	<entity class="com.catify.persistence.beans.PayloadCache">
		<named-query name="payloadCache_LoadByKey">
			<query>SELECT a FROM PayloadCache a WHERE a.beanKey = :param</query>
		</named-query>
		<named-query name="payloadCache_LoadAllKeys">
			<query>SELECT a.beanKey FROM PayloadCache a</query>
		</named-query>
		<attributes>
			<id name="id">
				<generated-value generator="system-uuid" />
			</id>
			<basic name="beanKey" optional="false"/>
			<basic name="beanValue" optional="false" >
				<column column-definition="TEXT"/>
			</basic>
		</attributes>
	</entity>
	<entity class="com.catify.persistence.beans.CorrelationCache">
		<named-query name="correlationCache_LoadByKey">
			<query>SELECT a FROM CorrelationCache a WHERE a.beanKey = :param</query>
		</named-query>
		<named-query name="correlationCache_LoadAllKeys">
			<query>SELECT a.beanKey FROM CorrelationCache a</query>
		</named-query>
		<attributes>
			<id name="id">
				<generated-value generator="system-uuid" />
			</id>
			<basic name="beanKey" optional="false" />
			<basic name="beanValue" optional="false" />
		</attributes>
	</entity>
	<entity class="com.catify.persistence.beans.CorrelationRuleCache">
		<named-query name="correlationRuleCache_LoadByKey">
			<query>SELECT a FROM CorrelationRuleCache a WHERE a.beanKey = :param
			</query>
		</named-query>
		<named-query name="correlationRuleCache_LoadAllKeys">
			<query>SELECT a.beanKey FROM CorrelationRuleCache a</query>
		</named-query>
		<attributes>
			<id name="id">
				<generated-value generator="system-uuid" />
			</id>
			<basic name="beanKey" optional="false" />
			<basic name="beanValue" optional="false" >
				<column column-definition="TEXT"/>
			</basic>
		</attributes>
	</entity>
	<entity class="com.catify.persistence.beans.TimerCache">
		<named-query name="timerCache_LoadByKey">
			<query>SELECT a FROM TimerCache a WHERE a.beanKey = :param</query>
		</named-query>
		<named-query name="timerCache_LoadAllKeys">
			<query>SELECT a.beanKey FROM TimerCache a</query>
		</named-query>
		<attributes>
			<id name="id">
				<generated-value generator="system-uuid" />
			</id>
			<one-to-one name="beanValue">
				<cascade>
					<cascade-all />
				</cascade>
			</one-to-one>
			<basic name="beanKey" optional="false" />
		</attributes>
	</entity>
	<entity class="com.catify.core.event.impl.beans.TimerEvent">
		<attributes>
			<id name="id">
				<generated-value generator="system-uuid" />
			</id>
			<basic name="instanceId" optional="false" />
			<basic name="time" optional="false" />
			<basic name="taskId" optional="false" />
		</attributes>
	</entity>
</entity-mappings>